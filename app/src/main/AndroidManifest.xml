<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.vvsai.rxjava">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".MyAppcation"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        tools:replace="android:icon, android:theme">
        <activity android:name=".excel.ExcelActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".FlatMapActivity" />
        <activity android:name=".rxbus.Test1Activity" />
        <activity android:name=".rxbus.Test2Activity" />
        <activity android:name=".rxbus.Test3Activity" />
        <activity android:name=".retrofit.Retrofit1Activity" />
        <activity
            android:name=".retrofit.Retrofit2Activity"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity android:name=".uitest.DrawerActivity" />
        <activity android:name=".uitest.TabActivity" />
        <activity android:name=".uitest.ItemListActivity"></activity>
        <activity
            android:name=".uitest.ItemDetailActivity"
            android:label="@string/title_item_detail"
            android:parentActivityName=".uitest.ItemListActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vvsai.rxjava.uitest.ItemListActivity" />
        </activity>
        <activity android:name=".uitest.SettingsActivity">

        </activity>
        <activity android:name=".uitest.ScrollingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>